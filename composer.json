{
    "name": "zoomyboy/laravel-nami",
    "description": "Authentication provider against NaMi for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Philipp Lang",
            "email": "philipp@zoomyboy.de"
        }
    ],
    "require": {
        "php": "8.1.*",
        "guzzlehttp/guzzle": "^6.3.1|^7.0",
        "laravel/framework": "^9.0",
        "laravel/ui": "^3.4",
        "spatie/data-transfer-object": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Zoomyboy\\LaravelNami\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zoomyboy\\LaravelNami\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "nunomaduro/larastan": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zoomyboy\\LaravelNami\\Providers\\NamiServiceProvider"
            ]
        }
    }
}
